{
    "name": "sanmai/shipandco-sdk",
    "description": "Ship&co API SDK",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "shipping provider",
        "PHP SDK",
        "delivery"
    ],
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com",
            "homepage": "https://www.alexeykopytko.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/sanmai/shipandco-sdk/issues"
    },
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "doctrine/annotations": "^1.14.4 || ^2.0",
        "guzzlehttp/guzzle": "^6.3 || ^7.9.3",
        "jms/serializer": "^3.24",
        "sanmai/json-serializer": "^0.1.1 || ^0.2.4",
        "sanmai/pipeline": "^6.14",
        "sanmai/sdk-common": "^0.5.6",
        "sanmai/version-info": "^0.2.2",
        "symfony/http-foundation": "^5.4.49 || ^6.4.21 || ^7.2.6"
    },
    "require-dev": {
        "colinodell/psr-testlogger": "^1.3",
        "doctrine/cache": "<2",
        "ergebnis/composer-normalize": ">=2.23",
        "friendsofphp/php-cs-fixer": "^3.75",
        "infection/infection": "^0.29.14",
        "laravel/framework": "^6.20.26 || ^8.40 || ^10 || ^12",
        "phan/phan": "^5.4.5",
        "php-coveralls/php-coveralls": ">=2.8.0",
        "phpstan/phpstan": "^2.1.17",
        "phpunit/phpunit": "^9.3",
        "psr/log": "^2 || ^3.0.2",
        "vimeo/psalm": "^6.7.0"
    },
    "conflict": {
        "doctrine/annotations": "<1.10.3"
    },
    "suggest": {
        "doctrine/cache": "For metadata caching",
        "monolog/monolog": "For advanced logging and debugging"
    },
    "autoload": {
        "psr-4": {
            "ShipAndCoSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CommonSDK\\Tests\\Common\\": "vendor/sanmai/sdk-common/tests/Common/",
            "Tests\\ShipAndCoSDK\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": true,
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    }
}
